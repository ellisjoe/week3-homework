<h4><%= link_to "Create New Place", "places/new", {:class => 'btn btn-primary btn-xs'} %></h4>

<table class="table table-striped">
<% @places.each do |place| %>
    <tr>
        <td><%= link_to place.title, "places/#{place.id}" %></td>
        <td><%= link_to "Delete", "places/#{place.id}/delete", {:class => 'btn btn-danger btn-xs'} %></td>
    </tr>
<% end %>
</ul>
